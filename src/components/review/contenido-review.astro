<style>




.review-card {
  display: flex;
  flex-direction: column;
  gap: 0.5rem;
  color:  white;
  max-width: 28rem;
  width: 80%;
  background-color: black;
  padding: 1.25rem;
  border-radius: 0.375rem;
  margin: 50px auto;
  box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);
  transition: transform 150ms ease-in-out;
     font-family: 'Rubik Variable', sans-serif;

}

.review-card:hover {
  transform: scale(1.05);
}

.dark .review-card {
  color: #fff;
  background-color: #171717;
}

.review-header {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
}

.review-author-date {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
}

.author, .date {
  font-size: 0.75rem;
  line-height: 1rem;
}

.review-title-rating {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
}

.title {
  font-size: 1.05rem;
  line-height: 1.75rem;
  font-weight: 700;
}

.rating {
  font-size: 0.75rem;
  line-height: 1rem;
}

.stars {
  display: flex;
  flex-direction: row;
}

.stars svg {
  height: 1rem;
  width: 1rem;
}

.stars svg:not(:last-child) {
  color: #facc15;
}

.stars svg:last-child {
  color: #fef08a;
}

.review-content {
  font-size: 0.875rem;
  line-height: 1.25rem;
}


.mas-reviws{
  position: relative;
  background: var(--color-tip2);
  color: black;
  width: 23px;
  height: 23px;
  border-radius: 50%;
  text-align: center;
}


.Titulo-de-la-pagina {
 font-family: 'Permanent Marker', cursive;
  color: var(--color-tip2);   
  background: black;
  width: 100%;
  text-align: center;
  padding-top: 20px;
  padding-bottom: 20px;
  position: relative;
  top: 0px;
  letter-spacing: 0.9px;
  font-size: 14px;
  
}



h2{
    filter: drop-shadow(1px 1px 5px rgb(255, 218, 54));
}
</style>


